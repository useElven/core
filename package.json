{
  "name": "@useelven/core",
  "version": "0.9.0",
  "description": "Core hooks for MultiversX React DApps",
  "license": "MIT",
  "author": "Julian Ä†wirko <julian.io>",
  "homepage": "https://www.useElven.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/useElven/core"
  },
  "keywords": [
    "multiversx",
    "xPortal",
    "blockchain",
    "sdk",
    "react",
    "nextjs",
    "dapp",
    "smart contracts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "tsup": {
    "entry": [
      "./src/index.tsx"
    ],
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "minify": "terser",
    "treeshake": true
  },
  "scripts": {
    "build": "rimraf build && tsup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/** --fix",
    "check-types": "tsc",
    "prettier": "prettier --write 'src/**/*.{js,ts,json}'",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@multiversx/sdk-core": "12.6.0",
    "@multiversx/sdk-extension-provider": "3.0.0",
    "@multiversx/sdk-hw-provider": "6.2.0",
    "@multiversx/sdk-native-auth-client": "1.0.4",
    "@multiversx/sdk-network-providers": "1.5.0",
    "@multiversx/sdk-wallet-connect-provider": "4.0.3",
    "@multiversx/sdk-web-wallet-provider": "3.0.0",
    "lodash.clonedeep": "4.5.0",
    "swr": "2.1.5",
    "valtio": "1.10.6"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "4.5.7",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "terser": "5.18.2",
    "tsup": "7.1.0",
    "typescript": "5.1.5"
  },
  "peerDependencies": {
    "react": "18.2.0"
  }
}
